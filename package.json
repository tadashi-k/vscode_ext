{
    "name": "editext",
    "displayName": "Edit Extension",
    "description": "",
    "version": "0.0.1",
    "publisher": "tadashi",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.deleteLine",
				"title": "Delete line",
				"category": "edit"
            },
            {
                "command": "extension.deleteEndOfLine",
                "title": "Delete end of line",
				"category": "edit"
            },
            {
                "command": "extension.yank",
                "title": "Yank",
				"category": "edit"
            },
            {
                "command": "extension.copyAndUnselect",
                "title": "Copy and Unselect",
				"category": "edit"
			},
			{
				"command": "extension.nextWord",
				"title": "Next word",
				"category": "move"
			}
		],
		"keybindings": [
			{
				"command": "extension.deleteLine",
				"key": "ctrl+y"
			},
			{
				"command": "extension.deleteEndOfLine",
				"key": "ctrl+k"
			},
			{
				"command": "extension.yank",
				"key": "ctrl+l"
			},
			{
				"command": "extension.nextWord",
				"key": "ctrl+f"
			}
		]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/copy-paste": "^1.1.30",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "copy-paste": "^1.3.0"
    }
}
